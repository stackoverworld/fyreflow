# Core image source and version managed by updater
FYREFLOW_CORE_IMAGE_REPOSITORY=ghcr.io/moiseencov/fyreflow-core
FYREFLOW_VERSION=latest

# Core runtime settings
FYREFLOW_RUNTIME_MODE=remote
DASHBOARD_API_TOKEN=change-me-core-api-token
DASHBOARD_SECRETS_KEY=change-me-secrets-key
CORS_ORIGINS=null,http://localhost:5173,http://127.0.0.1:5173
FYREFLOW_ENABLE_SCHEDULER=true
FYREFLOW_ENABLE_RECOVERY=true
FYREFLOW_ENABLE_REALTIME_WS=true
FYREFLOW_WS_PATH=/api/ws
FYREFLOW_WS_RUN_POLL_INTERVAL_MS=400
FYREFLOW_WS_HEARTBEAT_INTERVAL_MS=15000
FYREFLOW_UPDATER_BASE_URL=http://updater:8788
FYREFLOW_UPDATER_AUTH_TOKEN=change-me-updater-admin-token
FYREFLOW_UPDATER_TIMEOUT_MS=15000

# Updater settings
UPDATER_AUTH_TOKEN=change-me-updater-admin-token
UPDATER_GITHUB_OWNER=moiseencov
UPDATER_GITHUB_REPO=agents-dashboard
UPDATER_GITHUB_TOKEN=
UPDATER_CHANNEL=stable
UPDATER_IMAGE_REPOSITORY=ghcr.io/moiseencov/fyreflow-core
UPDATER_CORS_ORIGINS=*
UPDATER_AUTO_CHECK_INTERVAL_MS=300000
