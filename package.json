{
  "name": "agents-dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "electron/main.mjs",
  "scripts": {
    "dev": "concurrently -n api,web -c cyan,magenta \"tsx watch server/index.ts\" \"vite\"",
    "dev:api": "tsx watch server/index.ts",
    "dev:web": "vite",
    "dev:electron": "wait-on tcp:5173 && ELECTRON_RENDERER_URL=http://localhost:5173 nodemon --watch electron --ext mjs,cjs,js,json --exec \"electron .\"",
    "dev:desktop": "concurrently -k -n api,web,electron -c cyan,magenta,yellow \"bun run dev:api\" \"bun run dev:web\" \"bun run dev:electron\"",
    "start:api": "tsx server/index.ts",
    "start:desktop": "concurrently -k -n api,electron -c cyan,yellow \"bun run start:api\" \"electron .\"",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "typecheck": "tsc -b",
    "lint": "eslint ."
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "elkjs": "^0.11.0",
    "express": "^4.19.2",
    "lucide-react": "^0.468.0",
    "motion": "^12.34.2",
    "nanoid": "^5.0.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "electron": "^40.6.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "nodemon": "^3.1.13",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "wait-on": "^9.0.4"
  }
}
